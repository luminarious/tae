{
  "name": "tae",
  "version": "0.0.1",
  "description": "CSS framework",
  "main": "tae.styl",
  "style": "tae/css/tae.css",
  "scripts": {
    "build": "npm run build-clean && npm run build-stylus && npm run build-autoprefix",
    "build-clean": "rimraf css/*",
    "build-stylus": "stylus --sourcemap tae.styl --out css",
    "build-autoprefix": "postcss --use autoprefixer --map false --output css/tae.css css/tae.css",
    "deploy": "npm run build && npm run docs",
    "docs": "npm run docs-stylus && npm run docs-autoprefix",
    "docs-stylus": "stylus docs/bulma-docs.stylus docs/css/bulma-docs.css",
    "docs-autoprefix": "postcss --use autoprefixer --map false --output docs/css/bulma-docs.css docs/css/bulma-docs.css",
    "start": "npm run build-stylus -- --watch",
    "start-docs": "npm run docs-stylus -- --watch",
    "start-test": "npm run test-stylus -- --watch",
    "test-stylus": "stylus doc/bulma-test.stylus doc/css/bulma-test.css",
    "dev": "parcel docs/index.pug"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luminarious/tae.git"
  },
  "keywords": [
    "css",
    "stylus",
    "framework"
  ],
  "author": "luminarious",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luminarious/tae/issues"
  },
  "homepage": "https://github.com/luminarious/tae#readme",
  "files": [
    "css",
    "src",
    "tae.styl",
    "licence.md",
    "readme.md"
  ],
  "devDependencies": {
    "a11y.css": "^4.4.0",
    "autoprefixer": "^8.0.0",
    "changelog.md": "^1.1.0",
    "eslint": "^4.18.1",
    "parcel-bundler": "^1.6.2",
    "parcel-plugin-pug": "^0.2.5",
    "postcss-cli": "^5.0.0",
    "pug": "^2.0.0-rc.4",
    "pug-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "stylus": "^0.54.5"
  }
}
